{
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "extends": ["react-app", "standard", "plugin:prettier/recommended", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-max-props-per-line": [
      1,
      {
        "maximum": 1
      }
    ]
  }
}
