{
  "expo": {
    "name": "Papers Game - beta",
    "slug": "papers-game",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 15000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nabicas.papers"
    },
    "description": "The perfect game for your dinner party with friends!",
    "githubUrl": "https://github.com/sandrina-p/papers-game"
  }
}
