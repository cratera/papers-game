{
  "extends": ["standard", "plugin:prettier/recommended"],
  "plugins": ["prettier", "react", "react-native"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "react-native/react-native": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
